<form action="" method="POST">
    <div class="form-group m-4">
      <label for="logoUrl" class="my-2">Logo Url</label>
      <input name="logoUrl" type="url" class="form-control" id="logoUrl" placeholder="e.g. https:https://www.example.com/some.png">
    </div>
    <div class="form-group m-4">
        <label for="name" class="my-2">Name</label>
        <input name="name" type="text" class="form-control" id="name" placeholder="Flying Flunks">
    </div>
    <div class="form-group m-4">
        <label for="members" class="my-2">Members</label>
        <!-- <input  name="members" type="text" class="form-control" id="members" placeholder="Jon Snow, Daenerys, Melisandre, Arya Stark"> -->
        <textarea name="members" type="text" class="form-control" id="members" placeholder="Jon Snow, Daenerys, Melisandre, Arya Stark"></textarea>
    </div>
    <div class="d-flex justify-content-end m-4">
        <button type="submit" class="btn btn-outline-primary">Submit</button>
    </div>
  </form>